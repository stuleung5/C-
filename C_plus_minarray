#include <iostream>
using namespace std;

int main() {
    int ages[5];
    int i;
    int min;

    for (i = 0; i < 5;++i)
     {
        cin >> ages[i];
    }
   min=ages[0];
    for (i=0;i<5;i++)
    {
    if (min>ages[i])
    {
    min=ages[i];
    
    }
    }
    cout<<50-(min*0.5)<<endl;
    
    
    return 0;
}
