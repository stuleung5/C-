#include<iostream>
using namespace std;
int main() {
    int ages[5];
    int i;
    int minimum;
    for (i; i<5; ++i) {
        cin<<ages[i]<<endl;
    }  
    
    minimum=age[0];
    for (i; i<5; i++) {
        if (minimum>age[i]) {
            minimum=age[i];
            }
     }
     cout<<50-float(0.5*minimum)<<endl;
     return 0;
}
