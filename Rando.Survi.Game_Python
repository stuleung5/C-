n=0
for x in range(0,5):
	n+=x/13
class Player:
    def __init__(self, name, lives):
        self.name = name
        self._lives = lives

    def hit(self):
        	self._lives -= n
    #your code goes here
    @property
    def isAlive(self):
        if self._lives > 0:
            return True
p = Player("Cyberpunk77", int(input()))
i = 1
while True:
    p.hit()
    print("Hit # " + str(i))
    i += 1
    if not p.isAlive:
        print("Game Over")
        break
