#include <iostream>
using namespace std;

bool isPalindrome(int x) {
    //complete the function
    int num, rem, sum;
    num=x;
    while (x!=0) {
    	rem=x%10;
    	sum=res*10+rem;
    	x/=10;
    }
    if (num==sum) {
    	return true;
    }
    else {
    	return false;
    }
}
int main() {
    int n;
    cin >>n;

    if(isPalindrome(n)) {
        cout <<n<<" is a palindrome";
    }
    else {
        cout << n<<" is NOT a palindrome";
    }
    return 0;
}
